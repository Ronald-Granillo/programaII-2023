<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:background="#FFFFFF"
    tools:context=".lista_usuarios">

    <ListView
        android:id="@+id/ltsUsuarios"
        android:layout_width="411dp"
        android:layout_height="501dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="0dp">

    </ListView>

    <TextView
        android:id="@+id/txthome"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:background="#E91E63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtchat"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:layout_marginStart="96dp"
        android:background="#E91E63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtanadir"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_marginStart="192dp"
        android:background="#E91E63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtperfil"
        android:layout_width="122dp"
        android:layout_height="45dp"
        android:background="#E91E63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="68dp"
        android:layout_height="36dp"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/_37_2373015_logo_home_png_home_symbol_png_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txthome" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="47dp"
        android:layout_height="49dp"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="-5dp"
        android:src="@drawable/las"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/users"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>